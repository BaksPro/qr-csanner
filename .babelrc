{
  "presets": [
    "stage-0",
    "es2015",
    "react",
    "node7",
    [
      "env",
      {
        "targets": {
          "uglify": false
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
  ]
}





